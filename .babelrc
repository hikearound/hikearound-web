{
    "presets": [
        "next/babel"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components", {
                "minify": true,
                "pure": true,
                "ssr": true
            }
        ]
    ],
    "env": {
        "production": {
            "plugins": [
                "transform-remove-console", {
                    "exclude": ["error"]
                }
            ]
        }
    }
}
